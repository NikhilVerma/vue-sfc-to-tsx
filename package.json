{
  "name": "vuetsx",
  "version": "0.1.0",
  "description": "Convert Vue Single File Components (.vue) to Vue TSX (.tsx + .module.css)",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    }
  },
  "bin": {
    "vuetsx": "dist/cli.js"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "bun test",
    "build": "bun build src/index.ts src/cli.ts --outdir dist --target bun --external @vue/compiler-sfc --external @vue/compiler-dom --external @vue/compiler-core --external ai --external @ai-sdk/anthropic --external @ai-sdk/openai --external magic-string",
    "prepublishOnly": "bun test && bun run build",
    "lint": "oxlint src/",
    "playground": "bun run src/playground/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickvdv/vuetsx.git"
  },
  "keywords": [
    "vue",
    "tsx",
    "converter",
    "sfc",
    "jsx",
    "css-modules"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.5",
    "@vue/compiler-dom": "^3.5",
    "@vue/compiler-core": "^3.5",
    "ai": "^4",
    "@ai-sdk/anthropic": "^1",
    "@ai-sdk/openai": "^1",
    "magic-string": "^0.30"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxlint": "^1.46.0",
    "typescript": "^5.5"
  }
}
